# UART-TX - Connects to FT230 RX pin (connected to JX2)
set_property PACKAGE_PIN M14 [get_ports UART1_TX_0]
set_property IOSTANDARD LVCMOS25 [get_ports UART1_TX_0]

# UART-RX - Connects to FT230 TX pin (connected to JX2)
set_property PACKAGE_PIN M15 [get_ports UART1_RX_0]
set_property IOSTANDARD LVCMOS25 [get_ports UART1_RX_0]
